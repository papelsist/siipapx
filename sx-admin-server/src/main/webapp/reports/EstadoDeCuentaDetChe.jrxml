	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT CLIENTE,NOMBRE,SUC,TIPO_DOCUMENTO,FECHA,NUMERO,DOCUMENTO,SUM(SALDO) AS SALDO,PAG
FROM (
SELECT C.CLAVE AS CLIENTE,C.NOMBRE     ,S.CLAVE AS SUC,V.FECHA
,(CASE WHEN V.TIPO_DOCUMENTO ='NOTA_DE_CARGO' THEN 'CAR' WHEN V.TIPO_DOCUMENTO = 'CHEQUE_DEVUELTO' THEN 'CHE' ELSE 'VTA' END) TIPO_DOCUMENTO
,V.DOCUMENTO
,CASE WHEN V.TIPO_DOCUMENTO <>'NOTA_DE_CARGO' THEN (select x.numero from cobro_cheque x join cheque_devuelto h on(h.cheque_id=x.id) where h.cxc_id=v.id) 
ELSE (select x.numero from cobro_cheque x join cheque_devuelto h on(h.cheque_id=x.id) JOIN nota_de_cargo n on(h.nota_de_cargo_id=n.id) where n.cuenta_por_cobrar_id=v.id) END as NUMERO
,(V.TOTAL-IFNULL((SELECT  SUM(B.IMPORTE) FROM aplicacion_de_cobro B  WHERE B.cuenta_por_cobrar_id=V.ID AND B.FECHA<=$P{FECHA}),0)) AS SALDO
,CASE WHEN IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B JOIN COBRO Y ON(Y.ID=B.cobro_id) WHERE B.cuenta_por_cobrar_id=V.ID AND B.fecha<=$P{FECHA} AND Y.FORMA_DE_PAGO NOT IN('DEVOLUCION','BONIFICACION','PAGO_DIF','ESPECIE')),0)>0 THEN '*' ELSE '' END AS PAG
FROM cuenta_por_cobrar V JOIN CLIENTE C ON(V.CLIENTE_ID=C.ID) JOIN sucursal S ON(V.SUCURSAL_ID=S.ID)
WHERE V.FECHA BETWEEN '2017/01/01' AND $P{FECHA} AND V.TIPO='CHE' 
AND (V.TOTAL-IFNULL((SELECT  SUM(B.IMPORTE) FROM aplicacion_de_cobro B  WHERE B.cuenta_por_cobrar_id=V.ID AND B.FECHA<=$P{FECHA}),0))<>0
AND V.ID NOT IN (SELECT CXC_ID FROM juridico J WHERE J.CXC_ID=V.ID AND J.TRASPASO<=$P{FECHA})

UNION

SELECT C.CLAVE AS CLIENTE,C.NOMBRE      ,S.CLAVE AS SUC,V.FECHA,'BON' AS TIPO_DOCUMENTO,V.REFERENCIA,V.REFERENCIA
, -(V.IMPORTE-V.DIFERENCIA-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE  B.COBRO_ID=V.ID AND B.FECHA<=$P{FECHA}),0)) AS SALDO
,CASE WHEN IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B JOIN COBRO Y ON(Y.ID=B.cobro_id) WHERE B.cobro_id=V.ID AND B.fecha<=$P{FECHA} AND Y.FORMA_DE_PAGO NOT IN('DEVOLUCION','BONIFICACION','PAGO_DIF','ESPECIE')),0)>0 THEN '*' ELSE '' END AS PAG
FROM COBRO V  JOIN CLIENTE C ON(V.CLIENTE_ID=C.ID)  JOIN sucursal S ON(V.SUCURSAL_ID=S.ID)
WHERE V.FECHA BETWEEN '2017/01/01' AND $P{FECHA} AND V.TIPO='CHE' AND V.FORMA_DE_PAGO IN('BONIFICACION')
AND (V.IMPORTE-V.DIFERENCIA-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE  B.COBRO_ID=V.ID AND B.FECHA<=$P{FECHA}),0))<>0

) AS X 
GROUP BY CLIENTE,NOMBRE,SUC,TIPO_DOCUMENTO,FECHA,DOCUMENTO]]></queryString>

