	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="NomReporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["RELACION DE SALDOS A FAVOR POR DIA"]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA" isForPrompting="true" class="java.lang.String"/>
	<parameter name="SUCURSAL" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT A.ID AS ABONO_ID,A.FECHA,A.REFERENCIA,A.FORMA_DE_PAGO,L.CLAVE,L.NOMBRE,S.CLAVE AS SUC,S.NOMBRE AS SUCURSAL,A.SUCURSAL_ID AS ID_SUC,'' AS AUTDEP,A.PRIMERA_APLICACION
,(case when a.anticipo is true then "ANTICIPO" else ifnull((SELECT min(X.TIPO) FROM aplicacion_de_cobro B JOIN CUENTA_POR_COBRAR X ON(X.ID=CUENTA_POR_COBRAR_ID) WHERE A.ID=B.COBRO_ID  AND A.SUCURSAL_ID=$P{SUCURSAL} AND B.FECHA<=$P{FECHA} ORDER BY B.DATE_CREATED ASC),"ND") end) AS TIPO_VTA
,A.IMPORTE AS ABONO,(CASE WHEN A.DIFERENCIA_FECHA=$P{FECHA} THEN A.DIFERENCIA ELSE 0 END) AS DIFERENCIA,IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE A.ID=B.COBRO_ID AND B.FECHA<=$P{FECHA}),0) AS APLICADO
,(A.IMPORTE-(CASE WHEN A.DIFERENCIA_FECHA=$P{FECHA} THEN A.DIFERENCIA ELSE 0 END)-(IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE A.ID=B.COBRO_ID  AND A.SUCURSAL_ID=$P{SUCURSAL} AND B.FECHA<=$P{FECHA}),0))) AS SALDO
FROM cobro A JOIN SUCURSAL S ON (A.SUCURSAL_ID=S.ID) left join cobro_cheque x on(x.cobro_id=A.id) join cliente l on (l.id=a.cliente_id)
 WHERE   A.tipo IN ('CON','COD') AND A.SUCURSAL_ID=$P{SUCURSAL}  AND  A.forma_de_pago NOT IN('PAGO_DIF','BONIFICACION','DEVOLUCION') 
	AND ( (A.ANTICIPO IS false  AND date(A.PRIMERA_APLICACION) =$P{FECHA} AND (A.IMPORTE-(CASE WHEN A.DIFERENCIA_FECHA=$P{FECHA} THEN A.DIFERENCIA ELSE 0 END)-(IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE A.ID=B.COBRO_ID  AND A.SUCURSAL_ID=$P{SUCURSAL} AND B.FECHA<=$P{FECHA}),0)))>0 
		and (x.cambio_por_efectivo is false or x.id is null)	) or (a.anticipo is true and a.fecha=$P{FECHA}))]]></queryString>

