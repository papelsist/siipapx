	<queryString><![CDATA[SELECT I.PRODUCTO_ID,CURRENT_DATE AS FECHA,S.NOMBRE,P.LINEA_ID,P.CLASE_ID,(select C.clase FROM CLASE C WHERE C.ID=P.CLASE_ID) AS CLASE
,(SELECT L.LINEA FROM linea L WHERE  L.ID=P.LINEA_ID) AS LINEA,P.MARCA_ID,P.CLAVE,P.DESCRIPCION AS DESCRIPCION1,P.KILOS,P.GRAMOS,P.UNIDAD ,I.RECORTE
,CASE WHEN I.NACIONAL IS TRUE THEN 'NAL' ELSE 'IMP' END AS N_I,I.NACIONAL AS TIPO
,I.RECORTE_COMENTARIO,I.RECORTE_FECHA
FROM existencia I JOIN sucursal S ON(S.ID=I.SUCURSAL_ID) 
join producto p on(p.id=i.producto_id)
where p.inventariable is true and  i.anio=YEAR(CURRENT_DATE) and i.mes=MONTH(CURRENT_DATE)  AND s.id=(SELECT sucursal_id FROM app_config)
AND  RECORTE>0
GROUP BY I.PRODUCTO_ID]]></queryString>

